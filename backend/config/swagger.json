{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Swagger EmployeeList"
  },
  "host": "localhost:3002",
  "schemes": [
    "https"
  ],
  "basePath": "/api",
  "produces": [
    "application/json"
  ],
  "tags": [
    {
      "name": "user",
      "description": "Everything about employee"
    }
  ],
  "securityDefinitions": {
    "oauth2": {
      "name": "Authorization",
      "in": "header",
      "type": "apiKey"
    }
  },
  "paths": {
    "/auth/signup": {
      "post": {
        "summary": "signUp user",
        "parameters": [
          {
            "name": "user",
            "in": "body",
            "description": "Create new user",
            "required": true,
            "schema": {
              "type": "object",
              "default": {
                "name": "Uncle Bob",
                "email": "bob@test.com",
                "password": "test1234"
              }
            }
          }
        ],
        "tags": [
          "user"
        ],
        "responses": {
          "201": {
            "description": "Created"
          },
          "400": {
            "description": "Bad request"
          },
          "default": {
            "description": "Unexpected error"
          }
        }
      }
    }
  },
  "definitions": {
    "User": {
      "type": "object",
      "required": [
        "username",
        "email",
        "password"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      },
      "xml": {
        "name": "User"
      }
    }
  }
}